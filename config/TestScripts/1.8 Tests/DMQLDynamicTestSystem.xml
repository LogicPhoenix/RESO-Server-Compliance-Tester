<?xml version="1.0" encoding="UTF-8"?>
<RETSComplianceTest>
    <TestName>DMQLDynamicTestsSystem</TestName>
    <TestDescription>Test DMQL query language by using user's selections in TestParameters</TestDescription>
    <SearchTransaction>
        <TransactionLabel>DMQL Numeric Search - Minimum Value</TransactionLabel>
        <SearchType>${DMQL.Search.SearchType}</SearchType>
        <SearchClass>${DMQL.SearchClass}</SearchClass>
        <Query>${Search.System.RequiredFields}(${DMQL.System.Decimal.Minimum.Field}=${DMQL.System.Decimal.Minimum.Value}+)</Query>
        <QueryType>DMQL2</QueryType>
        <Count>1</Count>
        <Format>COMPACT-DECODED</Format>
        <Limit>2</Limit>
        <StandardNames>0</StandardNames>
        <TestEvaluators>
            <JavaClassName>com.realtor.rets.compliance.tests.dmql.DMQLSystemNumericResultsMin</JavaClassName>
        </TestEvaluators>
    </SearchTransaction>
