        <Count>1</Count>
        <Format>STANDARD-XML</Format>
        <Limit>2</Limit>
        <StandardNames>1</StandardNames>
        <TestEvaluators>
            <JavaClassName>com.realtor.rets.compliance.tests.dmql.DMQLStandardNumericResultsRange</JavaClassName>
