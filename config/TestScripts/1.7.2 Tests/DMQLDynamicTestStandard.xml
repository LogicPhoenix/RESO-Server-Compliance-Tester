    <SearchTransaction>
        <TransactionLabel>DMQL Character Search - OR</TransactionLabel>
        <SearchType>${DMQL.Search.SearchType}</SearchType>
        <SearchClass>${DMQL.SearchClass}</SearchClass>
        <Query>${Search.Standard.RequiredFields}(${DMQL.Standard.Character.AndOr.Field}=${DMQL.Standard.Character.AndOr.Value})|${Search.Standard.RequiredFields}(${DMQL.Standard.Decimal.Maximum.Field}=${DMQL.Standard.Decimal.Maximum.Value}-)</Query>
        <QueryType>DMQL2</QueryType>
        <Count>1</Count>
        <Format>STANDARD-XML</Format>
        <Limit>2</Limit>
        <StandardNames>1</StandardNames>
        <TestEvaluators>
            <JavaClassName>com.realtor.rets.compliance.tests.dmql.DMQLStandardCharResultsOR</JavaClassName>
